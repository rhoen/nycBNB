<section class="photos">
<% room._photos.forEach(function(photo) { %>
  <img src="<%=photo.thumb_url%>" alt="" />
<% }) %>
</section>
<form class="add-photo" action="index.html" method="post">
  <input type="file" class="photo-input" name="listing_photo[photo]" value="">
  <button class="add-photo-button">Save photo</button>
</form>
