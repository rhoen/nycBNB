<section class="photos clearfix">
<% room._photos.forEach(function(photo) { %>
  <article class="photo show-photo <% if (photo.primary_photo) { %>
    selected
<% } %>">
    <img src="<%=photo.small_url%>" data-id="<%=photo.id%>" alt="" />
    <button class="delete" data-id="<%=photo.id%>"></button>
  </article>
<% }) %>
</section>
